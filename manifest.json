{
  "author": "igorlogius",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "content-script.js"
      ],
      "matches": [
        "<all_urls>"
      ],
      "run_at": "document_idle"
    }
  ],
  "description": "Run external applications from the context (right click) menu",
  "icons": {
	"128": "icon.png"
  },
  "manifest_version": 2,
  "name": "handleWith",
  "options_ui": {
    "page": "options.html"
  },
  "permissions": [
    "menus",
    "activeTab",
    "storage"
  ],
  "version": "1.0.3"
}
